@using MudBlazor

<MudCard Elevation="2">
    <MudCardHeader>
        <CardHeaderContent>
            <MudText Typo="Typo.h6">Town Morale</MudText>
        </CardHeaderContent>
    </MudCardHeader>
    <MudCardContent>
        <MudText>@Morale</MudText>
    </MudCardContent>
</MudCard>

@code {
    [Parameter] public string Morale { get; set; } = string.Empty;
}